{
  "name": "@taura-ai/retrieval-core",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs --dts",
    "clean": "rimraf dist",
    "examples:hybrid": "tsx examples/hybrid-search.ts",
    "examples:rerank": "tsx examples/local-rerank.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "retrieval",
    "rerank",
    "pgvector",
    "taura",
    "hybrid-search"
  ],
  "devDependencies": {
    "@types/node": "^22.18.8",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@taura-ai/retrieval-core": "^0.1.0"
  }
}
